<p id="notice"><%= notice %></p>

<h1>Codes</h1>
<pre><%= @result %></pre>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th>Lang</th>
      <th>Status</th>
      <th>Output</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @codes.each do |code| %>
      <tr>
        <td><%= code.text %></td>
        <td><%= code.lang %></td>
        <td><%= code.status %></td>
        <td><%= code.output %></td>
        <td><%= link_to 'Show', code %></td>
        <td><%= link_to 'Edit', edit_code_path(code) %></td>
        <td><%= link_to 'Destroy', code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Code', new_code_path %>
